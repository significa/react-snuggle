{
  "name": "react-snuggle",
  "version": "1.0.3",
  "description": "Simple masonry-like css-based React Component",
  "keywords": [
    "grid",
    "masonry",
    "columns",
    "tiles",
    "pinterest",
    "css",
    "responsive",
    "react"
  ],
  "main": "dist/react-snuggle.cjs.js",
  "module": "dist/react-snuggle.es.js",
  "jsnext:main": "dist/react-snuggle.es.js",
  "scripts": {
    "dev": "npm run storybook",
    "build": "rollup -c",
    "deploy": "rm -fr ./dist && npm run build && npm publish",
    "test": "npm run lint && npm run flow",
    "test:jest": "jest",
    "test:ci": "cross-env NODE_ENV=test jest --ci --coverage",
    "test:changes": "cross-env NODE_ENV=test jest --changedSince master",
    "test:changes:watch": "cross-env NODE_ENV=test jest --changedSince master --watch",
    "test:update-snaps": "cross-env NODE_ENV=test jest -u",
    "staging": "storybook-to-ghpages",
    "storybook": "start-storybook -p 6006",
    "lint": "eslint 'src/**/*.js'",
    "flow": "flow",
    "build-storybook": "build-storybook",
    "publish": "rm -rf ./dist && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Significa/react-snuggle.git"
  },
  "author": "Significa Lda.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Significa/react-snuggle/issues"
  },
  "homepage": "https://github.com/Significa/react-snuggle#readme",
  "devDependencies": {
    "@significa/eslint-config-significa": "^1.0.0",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-storysource": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.4.0",
    "flow-bin": "^0.79.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-dom": "^1.12.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-testing-library": "^5.1.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "scrollreveal": "^4.0.0",
    "styled-components": "^3.4.5"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0"
  },
  "dependencies": {}
}
